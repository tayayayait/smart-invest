{
  "meta": {
    "name": "cmi-ai-invest-assist",
    "version": "1.0.0",
    "theme": "light",
    "source": "src/index.css",
    "format": "hsl"
  },
  "colors": {
    "primary": {
      "value": "hsl(217 91% 50%)",
      "foreground": "hsl(0 0% 100%)",
      "weak": "hsl(217 91% 95%)"
    },
    "background": "hsl(220 20% 98%)",
    "foreground": "hsl(222 47% 11%)",
    "card": {
      "value": "hsl(0 0% 100%)",
      "foreground": "hsl(222 47% 11%)"
    },
    "popover": {
      "value": "hsl(0 0% 100%)",
      "foreground": "hsl(222 47% 11%)"
    },
    "secondary": {
      "value": "hsl(220 14% 96%)",
      "foreground": "hsl(222 47% 11%)"
    },
    "muted": {
      "value": "hsl(220 14% 96%)",
      "foreground": "hsl(220 9% 46%)"
    },
    "accent": {
      "value": "hsl(217 91% 97%)",
      "foreground": "hsl(217 91% 40%)"
    },
    "semantic": {
      "success": {
        "value": "hsl(142 71% 45%)",
        "foreground": "hsl(0 0% 100%)",
        "weak": "hsl(142 71% 95%)"
      },
      "warning": {
        "value": "hsl(38 92% 50%)",
        "foreground": "hsl(0 0% 100%)",
        "weak": "hsl(38 92% 95%)"
      },
      "error": {
        "value": "hsl(0 84% 60%)",
        "foreground": "hsl(0 0% 100%)",
        "weak": "hsl(0 84% 96%)"
      },
      "info": {
        "value": "hsl(217 91% 60%)",
        "foreground": "hsl(0 0% 100%)",
        "weak": "hsl(217 91% 96%)"
      }
    },
    "neutral": {
      "0": "hsl(0 0% 100%)",
      "10": "hsl(220 20% 98%)",
      "20": "hsl(220 14% 96%)",
      "30": "hsl(220 13% 91%)",
      "40": "hsl(220 9% 84%)",
      "50": "hsl(220 9% 70%)",
      "60": "hsl(220 9% 56%)",
      "70": "hsl(220 9% 46%)",
      "80": "hsl(222 47% 25%)",
      "90": "hsl(222 47% 15%)",
      "100": "hsl(222 47% 11%)"
    },
    "border": "hsl(220 13% 91%)",
    "input": "hsl(220 13% 91%)",
    "ring": "hsl(217 91% 50%)",
    "cmiGradient": {
      "0": "hsl(220 9% 70%)",
      "25": "hsl(217 50% 60%)",
      "50": "hsl(217 70% 55%)",
      "75": "hsl(200 80% 50%)",
      "100": "hsl(170 75% 45%)"
    }
  },
  "typography": {
    "fontFamily": {
      "primary": "Pretendard",
      "fallback": "system-ui"
    },
    "scale": {
      "h1": { "sizePx": 24, "lineHeightPx": 32, "weight": 700 },
      "h2": { "sizePx": 20, "lineHeightPx": 28, "weight": 600 },
      "h3": { "sizePx": 18, "lineHeightPx": 24, "weight": 600 },
      "body": { "sizePx": 14, "lineHeightPx": 20, "weight": 400 },
      "caption": { "sizePx": 12, "lineHeightPx": 16, "weight": 400 },
      "mono": { "sizePx": 14, "lineHeightPx": 20, "weight": 400 }
    },
    "numeric": {
      "tabular": true,
      "lining": true
    }
  },
  "grid": {
    "baseUnitPx": 4,
    "spacingScalePx": [4, 8, 12, 16, 20, 24, 32],
    "columns": [4, 6, 12],
    "container": {
      "baseWidthDp": 360,
      "paddingMinPx": 16,
      "safeArea": true
    }
  },
  "motion": {
    "durationsMs": {
      "fast": 100,
      "medium": 200,
      "slow": 300
    },
    "easing": {
      "standard": "ease-in-out",
      "out": "ease-out"
    },
    "aiResponse": {
      "entry": { "type": "fade+slide-in", "durationMs": 300, "easing": "ease-out" },
      "update": { "type": "pulse", "durationMs": 200, "easing": "ease-out" }
    }
  },
  "shadows": {
    "sm": "0 1px 2px 0 hsl(0 0% 0% / 0.05)",
    "md": "0 4px 6px -1px hsl(0 0% 0% / 0.08), 0 2px 4px -2px hsl(0 0% 0% / 0.05)",
    "lg": "0 10px 15px -3px hsl(0 0% 0% / 0.08), 0 4px 6px -4px hsl(0 0% 0% / 0.05)",
    "card": "0 1px 3px 0 hsl(0 0% 0% / 0.08), 0 1px 2px -1px hsl(0 0% 0% / 0.05)"
  },
  "radii": {
    "basePx": 8
  }
}
